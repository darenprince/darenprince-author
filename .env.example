# Supabase project URL (e.g. https://your-project.supabase.co)
SUPABASE_URL=
# Optional alias for compatibility with Netlify's Supabase integration
SUPABASE_DATABASE_URL=${SUPABASE_URL}

# Public anon key used by the browser bundle
SUPABASE_ANON_KEY=
# Optional alias for SDKs that expect PUBLIC keys
SUPABASE_PUBLIC_ANON_KEY=${SUPABASE_ANON_KEY}

# Server-side secrets (never exposed to browsers)
SUPABASE_SERVICE_ROLE_KEY=
SUPABASE_JWT_SECRET=
